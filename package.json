  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "DARK_MODE=media expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "local-dev": "npx expo start --tunnel",
    "bump-version": "node scripts/bump-version.js",
    "bump-runtime": " node scripts/bump-runtime.js",
    "build-dev": "yarn bump-runtime && yarn bump-version && cross-env EAS_BUILD_PROFILE=development expo prebuild && eas build --profile development --platform android",
    "build-preview": "yarn bump-runtime && yarn bump-version && cross-env EAS_BUILD_PROFILE=preview expo prebuild && eas build --profile preview --platform android",
    "build-prod-android": "yarn bump-runtime && yarn bump-version && cross-env EAS_BUILD_PROFILE=production expo prebuild && eas build --profile production --platform android",
    "build-prod-ios": "yarn bump-runtime && yarn bump-version && eas build --profile production --platform ios",
    "publish-preview": "yarn bump-version && eas update --branch preview",
    "publish-dev": "yarn bump-version && eas update --branch development",
    "publish-prod": "yarn bump-version && eas build --platform android --profile production --auto-submit"
